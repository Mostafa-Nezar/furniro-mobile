{
  "expo": {
    "name": "furniro-mobile",
    "slug": "furniro-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/myicon.png",
    "scheme": "furniromobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/myicon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mostafanezar.furniromobile"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e8948e70-f5fa-4e9e-b7d6-c6ca06ce1f6d"
      }
    }
  }
}
