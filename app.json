{
  "expo": {
    "name": "furniro",
    "slug": "furniro-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/myicon.png",
    "scheme": "furniromobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
    "bundleIdentifier": "com.mostafanezar.furniromobile",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      },
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/myicon.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true,
      "edgeToEdgeEnabled": true,
      "package": "com.mostafanezar.furniromobile"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/myicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/myicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#B88E2F"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "webClientId": "866938789864-hfj30l2ktsbdb4t78r3cl1lj3p4vehmh.apps.googleusercontent.com",
          "iosUrlScheme": "com.googleusercontent.apps.866938789864-hfj30l2ktsbdb4t78r3cl1lj3p4vehmh"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0e9217d9-ef7b-48af-a80e-848d3090bb27"
      }
    },
    "owner": "mostafa-nezar"
  }
}
